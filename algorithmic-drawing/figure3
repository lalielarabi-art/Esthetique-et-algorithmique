void setup() {
  size(640, 400);
  v();
}

void v(){

int[] X= {0, 0, 2};
int[] Y= {0, 2, -1};

for (int  i=0; i<3; ++i){
  X[i]=X[i]*width/15;
  Y[i]=Y[i]*height/15;
}

for (int x=1; x<1101; x+=4){

  line(x/2.0,x/10.0*sin(x/20.0)+x/20.0+height/5.0,50*sin(width/(x*70.0))+height/2.0,x/4.0*sin(x/120.0)+width/5.0);
}
}
